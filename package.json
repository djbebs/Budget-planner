{
  "name": "savings-planner",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "file-saver": "^2.0.5",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "scripts": {
    "clean": "rimraf node_modules/.cache build",
    "predev": "npm run clean",
    "dev": "cross-env PORT=3000 HOST=localhost BROWSER=none WATCHPACK_POLLING=true react-scripts start",
    "dev:refreshed": "cross-env REACT_APP_UI=refreshed PORT=3000 HOST=localhost BROWSER=none WATCHPACK_POLLING=true react-scripts start",
    "start": "npm run dev",
    "build": "react-scripts build",
    "build:analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "bundle:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "optimize": "npm run clean && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
